# 1. Create a list of numbers from 1 to 10
numbers = []
for i in range(10):
    numbers.append(i+1);

# 2. Extract the first five elements
extracted_elements = numbers[:5]

# 3. Reverse the extracted elements
reversed_elements = extracted_elements[ : :-1]

# 4. Print both the extracted and reversed lists
print("Original list:", numbers)
print("Extracted list (first five elements):", extracted_elements)
print("Reversed list (of the extracted elements):", reversed_elements)
